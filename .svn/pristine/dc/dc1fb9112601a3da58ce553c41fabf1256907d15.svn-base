.insurer_premium1 .button_show{
	animation-name: buttons;
    animation-duration: .5s;
}
.insurer_premium1 .button_show:after{
	content:"without addon";
	position:absolute;
	color:#9a9898;
	bottom:-13px;
	left:5px;
	z-index:999;
	font-size:10px;
	font-weight:bold;
}
@-webkit-keyframes buttons {
    0% {left: 300%;}
    10% {left: 300%;transform:scale(2,2)}
	100%{left: 0%;}
}
.insurer_premium1 .val_btn:hover{
	box-shadow:0 0 10px 2px rgba(0,0,0,0.2);

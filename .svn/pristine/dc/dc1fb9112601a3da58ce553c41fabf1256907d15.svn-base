.range{
	display:inline;
	
}
.range label{
	font-weight:normal;
	text-align:center;
}
.range input{
	position:relative;
	top:-4px;
}
.range input[type="range"] {
    -webkit-appearance: none !important;
    -moz-appearance: none !important;
    -ms-appearance: none !important;
    -o-appearance: none !important;
    appearance: none !important;
	display:inline;
    background-color:#716b6b;
	box-shadow:0 15px 15px 0px rgba(123, 84, 84, 0.24);
    height: 2px;
	width:60%;
    cursor: pointer;
}
.range input[type="range"]::-webkit-slider-thumb {
    -webkit-appearance: none !important;
    -moz-appearance: none !important;
    -ms-appearance: none !important;
    -o-appearance: none !important;
    appearance: none !important;
	box-shadow:0 0 10px rgba(0, 0, 0, 0.5);
    width: 7px;
    height: 15px;
    text-align: center;
    white-space: nowrap;
    vertical-align: top;
    background-color:#000;
}
.range input[type="range"] {
    outline: none;
}
.range output{
	width:20%;
	display:inline;
}
.range_label{
	color:#000;
	padding-left:35%;
	font-size:12px;
}



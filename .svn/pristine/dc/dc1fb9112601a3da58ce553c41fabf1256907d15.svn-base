.premium_breakup{
	position: fixed;
    width: 100%;
    height: 100%;
    top: 0%;
    left: 0%;
    overflow-y: hidden;
    box-shadow: -3px 3px 5px 0 rgba(0,0,0,0.2);
    border: 1px solid #ddd;
    border-radius: 5px;
    background-color: rgba(4, 4, 4, 0.63);
    font-size: 12px;
    z-index: 999;
    /* display: none; */
}
.premium_breakup_inner{
	height: 350px;
    overflow-y: auto;
    max-width: 600px;
    background-color: #fff;
    margin: 0px auto;
    margin-top: 150px;
    border: 1px solid #4c4c4c;
    border-radius: 4px;
}
.over_flow{
	overflow-y:hidden;
}
.premium_breakup .breakup_title{
	padding-top: 10px;
    padding-bottom: 10px;
	text-align:center;
	border-bottom:1px solid #e60764;
	position:relative;
}
.premium_breakup .breakup_title h5{
	font-weight:700;
	color: #0674b3;
}
.remove_icon{
	position: absolute;
    right: 10px;
    top: 9px;
    font-size: 17px;
    color: #2196f3;
}
.remove_icon:hover{
	cursor:pointer;
	color:#fb3f46;
}
.breakup_title:after,.breakup_headings:after{
	border-top: 6px solid rgba(0,0,0,0.75);
	border-right: 6px solid transparent; 
	border-left: 6px solid transparent;
	content: '';
	height: 0;
    top: 100%;
    left:48%;
	width: 0px;
	position:absolute;
}

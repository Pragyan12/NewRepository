/*----------------ADDON COVER CHECKBOX----------------*/
/*------------------checkbox --------------------*/

/* Base for label styling */
#premium_addon_cover input[type="checkbox"]:not(:checked),
#premium_addon_cover input[type="checkbox"]:checked {
  position: absolute;
  left: -9999px;
}
#premium_addon_cover input[type="checkbox"]:not(:checked) + label,
#premium_addon_cover input[type="checkbox"]:checked + label {
  position: relative;
  padding-left: 1.95em;
  cursor: pointer;
}

/* checkbox aspect */
#premium_addon_cover input[type="checkbox"]:not(:checked) + label:before,
#premium_addon_cover input[type="checkbox"]:checked + label:before {
  content: '';
  position: absolute;
  left: 0; 
  top: 4px;
  width: 1em;
  height: 1em;
  background: #fff;
  border-radius: 2px;
  box-shadow: inset 0 1px 3px rgba(0,0,0,.1);
}
/* checked mark aspect */
#premium_addon_cover input[type="checkbox"]:not(:checked) + label:after,
#premium_addon_cover input[type="checkbox"]:checked + label:after {
  content: '✔';
  position: absolute;
  top: .1em; left: .1em;
  font-size: 1.3em;
  line-height: 0.8;
  color: #000;
  transition: all .2s; 
}
/* checked mark aspect changes */
#premium_addon_cover input[type="checkbox"]:not(:checked) + label:after {
  opacity: 0;
  transform: scale(0);
}
#premium_addon_cover input[type="checkbox"]:checked + label:after {
  opacity: 1;
  transform: scale(1);
}
#premium_addon_cover input[type="checkbox"]:checked + label:before {
  border: 1px solid #c2c4c5!important;
}
/* disabled checkbox */
#premium_addon_cover input[type="checkbox"]:disabled:not(:checked) + label:before,
#premium_addon_cover input[type="checkbox"]:disabled:checked + label:before {
  box-shadow: none;
  border-color: #bbb;
  background-color: #ddd;
}
#premium_addon_cover input[type="checkbox"]:disabled:checked + label:after {
  color: #999;
}
#premium_addon_cover input[type="checkbox"]:disabled + label {
  color: #aaa;
}
/* accessibility 
[type="checkbox"]:checked:focus + label:before {
  border: 2px solid blue;
}*/

/* hover style just for information */
#premium_addon_cover label:hover:before {
  border: 1px solid #c2c4c5!important;
  border-radius:2px;
}

				"engineCC"         :  details.vehicleInfo.engineCC,
				"fuelType"         :  details.vehicleInfo.fuelType,
				"rtoCode"          :  details.vehicleInfo.rtoCode.rtoCode,
				"rtoName"          :  details.vehicleInfo.rtoCode.rtoLocation,

.paymeny_btn:hover,.back_btn:hover{
/* 	background-color:#23234e; */
	color:#fff;
	opacity:0.8;
}
.paymeny_btn{
	background-color:#23234e;
	color:#fff;
	border:0px;
	border-radius:3px;
	/* box-shadow:0 0px 6px 0 rgba(0,0,0,0.4); */
}

.premium_summary_btn{
	
	padding-bottom:15px;
	margin-bottom:20px;
}


/*--------------PAYMENT DETAILS---------------*/

.accept_box{
	margin-top:20px;
}
.accept_box input{
	vertical-align:top;
}
.cheque_row{
	display:none;
}
#offline_payment{
	display:none;
}
.cheque_total_col .form-group{
	position: relative;
	margin-bottom:0px;
}
.cheque_total_col > div > div{
	padding-bottom:15px;
}
.val_error_pre{
	
}
.cheque_total_col .form-group .input-group-addon{
	  background-color: #2b1a3e;
	  color: #bfbfbf;
	  border: 0px;
	
}
.cheque_total_col .form-group .form-control {
    border-radius: 0px;
    background-color: rgba(255,255,255,1.0);
}
.cheque_total_col .form-group:hover .input-group-addon{
	color: #fff;
}
.cheque_total_col .form-group:before{
	content: attr(data-title);
	font-size:9px;
	position:absolute;
	top:-18px;
	right:0%;
	color:#c1c1c1;
	font-weight:700;
	z-index:100;
}
.termscon{
	max-width:400px;
	height:auto;
	background-color:#fff;
	border:1px solid #ddd;
	color:#000;
	margin:0px auto;
	border-radius:3px;
}
.termscon h3{
	position:relative;
}
.termscon h3 span{
	position:absolute;
	right:0%;
	top:0%;
	background-color:red;
	color:#fff;
	z-index:100;
	width:20px;
}
.termscon h3 span:hover{
	cursor:pointer;
}
/*-----------------media query for general and payment datails-------------------*/
@media screen and (min-width:500px) and (max-width:768px){
.premium_summary_body{
	margin-top:20px;
	margin-left:0px;
	margin-right:0px;
}
.premium_details .wrapper{
	margin:0px;
}
.gen_dae_col{
	border-right:0px !important;
	border-bottom:2px solid #fff;
}
}

@media screen and (min-width:200px) and (max-width:499px){
.premium_summary_body{
	margin-top:20px;
	margin-left:0px;
	margin-right:0px;
}
.addon_head > div{
	padding:0px;
}
.gen_dae_col{
	border-right:0px !important;
	border-bottom:2px solid #fff;
}
		
.premium_header_col > div{
	padding:0px;
}
.pre_sum_head2{
	padding:0px;
}
.cheque_total_col .row > div{
	width:100%;
}
}
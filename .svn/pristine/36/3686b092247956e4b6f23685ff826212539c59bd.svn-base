button:focus{
	outline:0;
}
/*end of header */

.gen_dae_col{
	border-right:2px solid #fff;
}
.premium_summary_body{
	margin-top:20px;
	
	margin-left:0;
	margin-right:0;
	
}
.gen_details .pre_sum_head1 h4{
	padding:5px 5px;
	/* background: linear-gradient(to bottom,#e91e63 0,#d84315 100%); */
	/* box-shadow:0 0 10px rgba(0,0,0,0.2); */
	color:#fff;
}
.payment_detail_head{
	padding:5px 5px;
	color:#fff;
}
.pre_sum_head2 p{
	color:#fff;
	font-weight:normal;
	margin-bottom: 5px;
	 word-break: keep-all;
}
.cover_detail_head{
	font-weight:700;
	padding:5px;
}
.header_margin{
	width:40px;
	margin:0px auto;
	border-bottom:2px solid #f0ffff;
}
.gen_details .idv_ncb{
	box-shadow:0 0 10px rgba(0,0,0,0.2); 
	margin-top:20px;
	background-color:#eae9e9;
	
	color:#035dbd;
}
.gen_image_span{
	position: absolute;
    right:2px;
    top: 2px;
	font-size:14px;
	color: #000;
	text-shadow:1px 1px #777;
  
}
.gen_image,.gen_ncb_idv{
	padding:8px 8px;
}
.gen_ncb_idv{
	background-color:#7196a7;
    color: #fff;
	
}
.gen_ncb_idv .ncb{
	padding-top:8px;
}
.rs{
	position:absolute;
	left:0%;
}
.gen_ncb_idv .idv{
	padding-bottom: 8px;
	border-bottom:1px solid #000;
}
.premium_summary_body .customer_detail{
	margin-top:20px;
	font-size:14px;
}
.premium_summary_body .customer_detail .row{
	padding-top:2px;
	padding-bottom:2px;
	margin-bottom:10px;
}
.premium_summary_body .customer_detail .row  span{
	position:absolute;
	right:0px;
}
.premium_summary_body .customer_detail .row  span.glyphicon{
	top:3px;
	color:#080808;
}
.premium_summary_body .customer_detail .addon_head{
    margin-top: 10px;
    margin-bottom: 5px;
    box-shadow: 0 8px 6px -6px black;
    /* border-radius: 10px; */
    background-color: aliceblue;
    padding: 0px;
	
}
.addon_head{
	border:1px solid #ddd;
}
.addon_head > div{
	 border: 1px solid #d0cfcf;
	     padding: 4px
}
.addon_head > div > div{
	padding:0px;
}
.addon_head p{
	color:#000;
	    font-weight: 600;
}
.addon_head .coverdetailvalues{
    border-left: 1px solid #777676;
    text-align: left;
    padding-left: 5px;
    color: #000;
}
.additionalCoverList_length{
	color: #f90404;
    font-weight: 500;
    font-size: 14px;
        padding-top: 5px;
    padding-bottom: 10px;
}
.coverdetailvalues > div{
	display:inline-block;
}
.coverdetailvalues > div div{
	padding-top: 5px;
    padding-bottom: 10px;
}
.premium_summary_body .customer_detail .addon_head h4{
	color:#fff;
	font-size:14px;
	font-weight:bold;
	padding-bottom:3px;
}
.addon_head_margin{
	border-bottom:1px solid #fff;
	width:40px;
}
.premium_summary_body .customer_detail .addon_info p{
	/* width:30px; */
	font-size:12px;
	box-sizing:content-box;
	/* background-color:#444; */
	display:inline-block;
	padding:8px;
	color:#fff;
	/* border-right:3px solid #444;  */
	position:relative;
	margin:3px;
}
.premium_summary_body .customer_detail .addon_info ol{
	color:#fff;
}
.premium_summary_body .customer_detail .addon_info ol li{
	list-style-type:disc;
	display:inline-block;
	padding:5px;
}
.premium_summary_body .customer_detail .addon_info ol li:after{
	content:",";
	position:absolute;
	
}
.premium_summary_body .customer_detail .addon_info ol li:last-child:after{
	content:".";
	position:absolute;
}
.additonal_covers{
	padding-left:0px;
}
.premium_summary_body .row .additional_cover_name span{
	color:#fff;
	font-size:14px;
}
#premiumsummary_dummy{
	width:100%;
	height:20px;
}

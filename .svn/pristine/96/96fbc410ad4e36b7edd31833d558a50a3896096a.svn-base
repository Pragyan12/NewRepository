#zoomsection{
	position:fixed;
	width:100%;
/* 	height:400px; */
	    background-color: rgba(4, 4, 4, 0.46);
	top:0%;
	z-index:9999;
	display:table;
}

#zoomsection #zoomsection_inner{
	text-align:center;
	display:table-cell;
	vertical-align:middle;
	position:relative;


	
}
#zoomsection #closezoom{
	position:absolute;
	right:5%;
	top:5%;
	font-size:26px;
	color:#ddd;
}
#zoomsection #closezoom:hover{
	cursor:pointer;
}
#zoomsection #zoomsection_inner img{
		max-width:700px;
		margin:0px auto;
	}
/* 	END ZOOM IN */

	/*Start of Home page*/
	
.home .home_main{
	width:100%;
	min-height:523px;
	position:relative;
	z-index:1;
}
.home .back_btn{
	position: absolute;
    display: inline;
    z-index: 100;
    top: 6px;
    left: 0px;	
}
.home .back_btn a{
	padding: 10px 15px;
    
    
}
.home .back_btn a span{
	    font-size: 16px;
		color: #000000;
}
.home .back_btn a .glyphicon{
	top:3px;
}
.home .back_btn a:hover span{
	color:#fff;
}
.home .home_main .home_main_row_product{
	padding-top:120px;
	height:300px;
	position:relative;
}
.product_name{
	color:#fff;
}
 .home .home_main .home_main_row_product .total_product  > div:nth-child(1){
	position:relative;
	 top:-150px;
	opacity:0.0; 
	height:110px;
	animation:marker1 .5s cubic-bezier(.5,.5,1.0,1.0) forwards;
}
@keyframes marker1{
	100%{
		opacity:1.0; 
		top:0px;
	}
}
.home .home_main .home_main_row_product .total_product  > div:nth-child(2){
	position:relative;
	 top:-150px;
	opacity:0.0; 
	height:140px;
	animation:marker2 .5s .2s cubic-bezier(.5,.5,1.0,1.0) forwards;
}
@keyframes marker2{
	100%{
		opacity:1.0; 
		top:0px;
	}
}

.home .home_main .home_main_row_product .total_product  > div:nth-child(3){
	position:relative;
	 top:-150px;
	opacity:0.0; 
	height:110px;
	animation:marker3 .5s .3s cubic-bezier(.5,.5,1.0,1.0) forwards;
}
@keyframes marker3{
	100%{
		opacity:1.0; 
		top:0px;
	}
}

.home .home_main .home_main_row_product .total_product  > div:nth-child(4){
	position:relative;
	 top:-150px;
	opacity:0.0; 
	height:110px;
	animation:marker4 .5s .4s cubic-bezier(.5,.5,1.0,1.0) forwards;
}
@keyframes marker4{
	100%{
		opacity:1.0; 
		top:0px;
	}
}
 .home .home_main .home_main_row_product .total_product .product_img div img{
	width:142px;
	transition:all .2s ease-out;
	
}
 .home .home_main .home_main_row_product .total_product .product_img div img:hover{
	width:150px;
	cursor:pointer;
}
.home .selected{
	position:absolute;
	top:80px;
	left:38%;
	width:20px;
	height:20px;
	color:#ec4518;
	
	
}
.home .card_input_col .form-control{
	background-color: #02bcd2;
	border:0px;
	color:#fff;
}
.home .card_input_col .input-group-addon{
	padding-left:10px;
	padding-right:10px;
}
.home .card_input_col .form-control::-webkit-input-placeholder { /* Chrome/Opera/Safari */
  color: #fff;
  opacity:0.8;
  font-size:11px;
}
.home #vec_num_value,.home #aad_num_value{
	text-transform:uppercase;
}

.home .home_main .home_main_row_product .total_product  > div:hover .img_circle{
	opacity:1.0;
}
.home .img_circle{
	border-radius:50%;
	height :15px;
	width :15px;
	position: absolute;
	left:33%;
	top:105px !important;
	transform :rotateX(55deg);
	z-index:-1;
	opacity:0.0;
}

.home .img_circle:before{
	content: "";
    border-radius: 50%;
    height: 40px;
    width: 40px;
    position: absolute;
	animation: pulsate .8s ease-out;
    animation-iteration-count: infinite;
}
.home .img_circle:after{
	content: "";
    border-radius: 50%;
    height: 40px;
    width: 40px;
    position: absolute;
	animation: pulsate .8s ease-out .4s;
    animation-iteration-count: infinite;  
}

@keyframes pulsate{
	
  0%{
    transform:scale(0.1, 0.1);
    opacity: 0.0;
  }
  50%{
    opacity: 1.0;
  }
  100%{
    transform: scale(1.2, 1.2);
    opacity: 0;
  }
}




.home .comlogo img{
	margin:0px auto;
}
.home .product_img .img_circle:after{
	border:3px solid #f4ec70;
}
.home .product_img .img_circle:before{
	border:3px solid #f4ec70;
}

.home .home_main .text_box_row{
	position:relative;
	/* display:none; */
	background-color: #444;
    padding-top: 10px;
    padding-bottom: 35px;
	
}
.home .home_main .text_box_row  .product_name{
	margin-top:10px;
	margin-bottom:20px;
	font-size:16px;
	color:#fff;
}
.home .down_arrow{
	border-top: 6px solid rgba(255,255,255,1);
	border-right: 6px solid transparent; 
	border-left: 6px solid transparent;
	content: '';
	height: 0;
    top: 1%;
    left:50%;
	width: 0px;
	position:absolute;
}
.home .home_main .text_box_row .card_input{
	
}
.home .home_main .text_box_row .card_input > div{
	padding:0px;
}
.home .home_main .text_box_row .card_input div img:nth-child(2),.home .home_main .text_box_row .card_input div img:nth-child(3){
	display:none;
	margin-top:4px;
}
.home .home_main .text_box_row .card_input div .vec_aad_num{
	margin-bottom:0px;
	
}
.home .home_main .text_box_row .card_input div .vec_aad_num .vec_aad_num_value{
	text-transform:uppercase;
	text-align:center;
}

.home .home_main .text_box_row .home_submit .home_button{
	background-color:#c13f3d;
    color: #fff;
	font-weight:bold;
    padding: 10px 10px;
	/* padding-left:25px; */
	border:0px;
	box-shadow:0 0 10px rgba(0,0,0,0.5);
	margin-top:10px; 
}
.home .home_button img:focus{
	outline:0;
}
.home .home_submit button:active:focus{
	outline:0;
}

.home .text_box_row .bottom_link{
	margin-top:10px;
}
.home .bottom_link a{
	color:#f1f3f2;
	text-decoration:underline;
}

.home #errmsg{
	position:absolute;
	z-index:999;
	left:50%;
	top:70%;
	color:#fff;
}	
	/*End of Home page*/
	

	/*Start of Falist page*/
.entire{
	width:100%;
	height:auto;
	
}
.wrapper{
	
	margin:0px auto 0px auto;
	
}
.totalpage{
	width:100%;
	height:auto;
	/*box-shadow:0 0 5px 0 rgba(0,0,0,0.5);*/
	
	background-color:#2e80ab; 
}


.falist .falist_slider{
	padding:0px;
	
}
.falist  .slider_row{
	margin:0px;
}
.falist  .slider_col{
	padding:0px;
	position:relative;
	padding-top: 20px;
}
.carousel-control.left{
	background-image:none;
}
.carousel-control.right{
	background-image:none;
}
.slide_arrow{
	color:#000;
}
.falist  .tran_popup_main{
	position:absolute;
	min-width:200px;
	height:auto;
	top:10px;
	background-color:#777;
	z-index:100;
	right:50px;
	border-radius:4px;
	box-shadow:1px 2px 6px 0 rgba(0,0,0,0.3);
	font-size:10px;
	font-weight:bold;
	
}
.falist  .tran_popup_inner{
	width:inherit;
	height:inherit;
	position:relative;
	background-color:#0662ad;
	color:#fff;
	border-radius:4px;
}
.falist  .tran_popup_inner h5{
	margin:0px;
	padding-top:10px;
	padding-bottom:10px;
	background-color:#edf258;
	color:#000;
	border-top-left-radius: 4px;
    border-top-right-radius: 4px;
	position:relative;
	font-weight:bold;
	
}
.falist  .tran_popup_order{
	
	position:relative;
	border-right:3px solid #edf258;
}
.falist  ol.tran_popup_order{
	margin:0px;
}
.falist  .tran_popup_order > li{
	padding:5px;
	position:relative;
	top:30px;
	 border-bottom:1px solid #bbb8b8;  
}
.falist  .tran_popup_order > li:after{
	content:"";
	width:50px;
	position:absolute;
	margin:0px auto;
}
.falist  .tran_popup_order li ul li{
	list-style-type:none;
	display:inline;
}
.falist  .tran_popup_order li a{
	color:#fff;
}
.falist  .close_tran_popup{
	position:absolute;
	right:-5px;
	top:-3px;
	font-size:16px;
	color:#000;
}
.falist .close_tran_popup:hover{
	cursor:pointer;
}
.falist .falist_arrow{
	width:0px;
}

.falist .slide_images{
	padding-left:0px;
	padding-right:0px;
	padding:3px;
	border:3px solid #ddd;
	min-height:250px;
}
.falist .slide_images img{
	width:100%;
	height:250px;
	
}
.falist .slide_images img:nth-child(1){
	border-right:2px solid #ccc;
}
.falist .slide_images img:nth-child(2){
	border-right:2px solid #ccc;
}
.falist .falist_arrow_glyleft{
	margin-left:20px !important;
	color:#444;
}
.falist .falist_arrow_glyright{
	margin-right:20px !important;
	color:#444;
}
.falist .carousel-caption{
	color:#444;
}
.falist .carousel-indicators li{
	border:2px solid #f44336;
}
.falist .carousel-indicators .active {
	background-color:#f44336;
}
.falist .falist_main{
	
	width:100%;
	min-height:100px;
	position:relative;
	z-index:1;
}
.falist .falist_main_row{
	margin-top:50px;
	margin-bottom:80px;	
}
.falist .motor_col,.falist .sme_col{
	min-height:60px;
}
.falist .falist_sub_col{
	min-height:120px;
	border:1px solid #0293a4;
	
}
.falist .falist_sub_col div{
	padding-top:10px;
	padding-bottom:10px;
}
.falist .falist_sub_col h4{
	font-weight:400;
}
.falist .falist_sub_col a{
	color:#fff;
}
.falist .falist_sub_col a:hover{
	color:#fff;
	text-decoration:none;
}
.falist .motor_col:hover,.sme_col:hover{
	cursor:pointer;
	box-shadow:0 0 10px 0 rgba(0,0,0,0.3);
	opacity:0.9;
	
}
.falist .motor_col:hover a{
	
}
.falist .item_images img{
	margin:0px auto;
}
.falist .falist_sub_col p{
	font-weight:700;
}
.falist .falist_sub_col .item_images{
	min-height:93px;
}
.falist .motor_col{
	background-color:#ff6200;
	color:#fff;
}
.falist .sme_col{
	background-color:#282725;
	color:#fff;
}
.falist .motor_products,.falist .sme_products{
	position:relative;
	min-width:60px;
	margin:0px auto;
	background-color:#55585f;
	border-top: 4px solid #55585f;
	border-right: 4px solid #55585f;
	border-left: 4px solid #55585f;
	border-bottom:4px solid #55585f;
	padding-top:30px;
	padding-bottom:30px;
	text-align:center;
	vertical-align:middle;
	box-shadow: 0 10px  20px 0 rgba(0,0,0,0.3);
	transition:all .1s .1s;
	
}
.falist .motor_products p,.falist .sme_products p{
	vertical-align:middle;
	font-size:22px;
	font-style:normal;
	color:#fff;
	width:100px;
	margin:0px auto;
	position:relative;
	
}
.falist .motor_products p:after,.falist .sme_products p:after{
	content:"";
	position:absolute;
	right:0px;
	border-right:2px solid #fff;
	height:20px;
	transition:all .3s;
}
.falist .motor_icon,.falist .sme_icon{
	position:absolute;
	font-size:20px;
	left:-15px;
	top:0px;
	color:#d8ff00;
	text-align:center;	
}
@keyframes falist_animate{
	0%{transform:rotate(120deg)scale(0,0);}
	50%{transform:rotate(240deg) scale(1,1);color:#d8ff00;}
	100%{transform:rotate(360deg) scale(0,0);}
}
.falist .motor_products:hover,.falist .sme_products:hover{
	cursor:pointer;
	border-right:4px solid #e91e63;  
	border-left:4px solid #e91e63; 
	border-top:4px solid #e91e63; 
	border-bottom:4px solid #e91e63; 
	
}
.falist .motor_products:hover .motor_icon,.falist .sme_products:hover .sme_icon{
	color:#e91e63;
	animation-name:falist_animate;
	animation-duration:.3s;
	text-align:center;
}
.falist .motor_products:hover  p:after,.falist .sme_products:hover  p:after{
	right:-40px;
	border-right:4px solid #fff;
	height:30px;
	opacity:0.0;
}
.falist a:hover{
	text-decoration:none;
}	
	/*End of Falist page*/
	

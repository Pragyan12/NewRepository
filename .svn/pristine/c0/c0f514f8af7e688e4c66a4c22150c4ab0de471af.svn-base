/*media queries for login ,falist ,home page are at the bottom*/



/*-----------------------------------------------------*/
/*start of login page*/
div.login_container{
	max-width:400px;
	height:auto;
	margin:100px auto 45px auto;
	background-color:#ddd;
	padding:30px;
	padding-bottom:20px;
	box-shadow: 0px 0px 20px 0 rgba(0,0,0,0.2);
	position:relative;
}
.login_container .login_logo img{
	margin:0px auto;
	margin-bottom:20px;
}
.login_container .userform label{
	color:#29759e;
}
.login_error{
	color:#f14611;
}
.login_container .userform .form-group{
	margin-bottom:0px;
}
.login_container .userform .username_field{
	margin-bottom:10px;
}
.login_container .userform .password_field{
	margin-bottom:10px;
}
.login_container .form-group input:focus{
     outline-width: 0;
	 outline: none ;
	 outline: 0 ;
	 box-shadow:none;
	 
}
.login_container .userform .form-control{
	border-radius:0px;
	
	border:1px solid #29759e;
	background-color: #ddd;
}
.login_container .userform .input-group-addon{
	background-color:#29759e;
	color:#fff;
	border:0px;
	border-radius:0px;
}
.login_container .clear{
	display:none;
	position:absolute;
	right:15%;
	top:25%;
	z-index:999;
	
}
.login_container .clear:hover{
	cursor:pointer;
}
.login_container .login_button{
	margin-top:20px;
}
.login_container .login_button button{
	background-color:#29759e;
	color:#b4d9f7;
	border:0px;
	border-radius:0px;
	font-size:14px;
	font-weight:bold;
	width: 100%;
}
.login_container .login_button button:hover{
	background-color:#29759e;
	color:#fff;
}
.login_container .login_button button:focus{
	outline:0;
}
.login_container .forgot_password{
	text-align:right;
	margin-top:10px;
	
}
.login_container .agree_terms{
	text-align:center;
	margin-top:10px;
	
}
.login_container form a{
	color:#0415f7;
}
.login_container form a:hover{
	cursor:pointer;
}
.forgot_password_popup{
	width: 500px;
	border-radius:4px;
	background-color:#fff;
	box-shadow: 0px 0px 20px 0 rgba(0,0,0,0.2);
	height:400px;
	overflow-y:auto;
	position:absolute;
	z-index:200;
	top:0%;
	left:-10%;
	border:1px solid #000;
	
}
.forgot_password_container{
	padding:0px;
}
.forgot_password_popup .forgot_password_header{
	background-color:#ddd;
	padding-top:20px;
	padding-bottom:20px;
	text-align:center;
}
.forgot_password_popup .forgot_password_close{
	position:absolute;
	right:5%;
	top:5%;
	z-index:100;
	font-weight:700;
	font-size:18px;
	transition:all .2s ease-in;
}
.forgot_password_popup .forgot_password_close:hover{
	color:red;
	cursor:pointer;
}
.forgot_password_forms{
	padding:10px;
	margin:20px;
	border:1px solid #bbb;
	border-radius:3px;
}
.forgot_password_popup .forgot_password_forms div{
	width:100%;
	clear: both;
	padding-top:5px;
	padding-bottom:5px;
}
.forgot_password_popup .forgot_password_forms div label{
	width:40%;
	float:left;
	text-align:right;
	font-weight:400;
	padding-right:15px;
	margin-top:15px;
}
.forgot_password_popup .forgot_password_forms div .form-control{
	width:45%;
	
}
.forgot_password_popup .forgot_password_forms div p{
	width:60%;
	display:inline-block;
	margin-top:15px;
	text-align:left;
}
.forgot_button{
	margin-top:15px;
}
.forgot_password_popup .forgot_password_forms div .form-control{
	box-shadow: none;
}
.forgot_password_popup .forgot_password_forms div input {
  border: none;
  outline: none;
  padding: 10px 0;
  border-radius:0px;
  border-bottom: solid 1px #00b9ff;
  -webkit-transition: all 0.3s cubic-bezier(0.64, 0.09, 0.08, 1);
  transition: all 0.3s cubic-bezier(0.64, 0.09, 0.08, 1);
  background: -webkit-linear-gradient(top, rgba(255, 255, 255, 0) 96%, #00b9ff 4%);
  background: linear-gradient(to bottom, rgba(255, 255, 255, 0) 96%, #00b9ff 4%);
  background-position: -200px 0;
  background-size: 200px 100%;
  background-repeat: no-repeat;
  color: #0e6252;
}
.forgot_password_popup .forgot_password_forms div input:focus {
 box-shadow: none;
 outline: none;
 background-position: 0 0;
}

.terms_popup{
	width: 500px;
	border-radius:4px;
	background-color:#fff;
	box-shadow: 0px 0px 20px 0 rgba(0,0,0,0.2);
	height:400px;
	overflow-y:auto;
	position:absolute;
	z-index:200;
	top:0%;
	left:-10%;
	border:1px solid #000;
}
.terms_container{
	padding:0px;
}
.terms_popup .terms_header{
	position:fixed;
	background-color:#ddd;
	width:480px;
	padding-top:5px;
	padding-bottom:5px;
	text-align:center;
}
.terms_popup .terms_close{
	position:absolute;
	right:0%;
	top:0%;
	background-color:#d0cdcd;
	z-index:100;
	font-weight:700;
	font-size:18px;
	transition:all .2s ease-in;
	padding-top: 3px;
    padding-bottom: 5px;
    padding-left: 3px;
    padding-right: 3px;
}
.terms_popup .terms_close:hover{
	color:#fff;
	cursor:pointer;
	background-color:#f55151;
}
.terms_popup .terms_main{
	padding:5px;
	margin-top:30px;
}
.terms_popup .terms_main h6{
	font-weight:700;
	padding-top:10px;
	padding-bottom:10px;
}
.terms_popup .terms_main p{
	padding-left:10px;
	padding-bottom:5px;
}
	/*End of login page*/
		
